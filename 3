#include <iostream>
#include <ctime>
using namespace std;
void print_array(int** mas, int n)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
		{
			cout << mas[i][j] << " ";
		}
		cout << endl;
	}
	cout << endl;
}
void random_array(int** mas, int n)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
		{
			mas[i][j] = rand() % 90 + 10;
		}
	}
}
void min(int** mas, int n)
{
	for (int i = n-1; i >= 0; i--)
	{
		for (int j = n-1; j >= 0; j--)
		{
			cout << mas[i][j] << " ";
		}
		cout << endl;
	}
}
int main()
{
	srand(time(0));
	int n;
	cin >> n;
	int** mas = new int* [n];
	for (int i = 0; i < n; i++)
	{
		mas[i] = new int[n];
	}
	random_array(mas, n);
	print_array(mas, n);
	min(mas, n);
}
